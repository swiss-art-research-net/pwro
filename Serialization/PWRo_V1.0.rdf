    <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P11i_participated_in"/>
  </rdf:Property>
  <rdf:Property rdf:about="https://ontology.swissartresearch.net/pwro/WP3i_exercised_authority_of">
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E39_Actor"/>
    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property relates an instance of sovereignty to an actor, individual or group, who wielded that sovereignty.</rdfs:comment>
