    <rdfs:range rdf:resource="https://ontology.swissartresearch.net/pwro/WE1_Exercise_of_Authority"/>
  </rdf:Property>
  <rdf:Property rdf:about="https://ontology.swissartresearch.net/pwro/WP2i_was_subject_to_authority_of">
    <rdfs:domain rdf:resource="https://ontology.swissartresearch.net/aaao/ZE36_Persons"/>
    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property connects an instance of exercise of authority to the persons individually or collectively over whom it exercised authority.</rdfs:comment>
    <rdfs:label xml:lang="en">was subject to authority of</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WP2i</skos:notation>
