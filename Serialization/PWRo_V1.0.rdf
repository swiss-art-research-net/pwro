    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WP16</skos:notation>
    <rdfs:domain rdf:resource="https://ontology.swissartresearch.net/pwro/WE6_Attack"/>
    <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P12_occurred_in_the_presence_of"/>
    <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P14_carried_out_by"/>
  </rdf:Property>
  <rdf:Property rdf:about="https://ontology.swissartresearch.net/pwro/WP1i_was_geographic_scope_of">
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property relates an instance of exercise of authority to the location over which it projects this authority.</rdfs:comment>
    <rdfs:label xml:lang="en">was geographic scope of</rdfs:label>
