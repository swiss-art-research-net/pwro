    <rdfs:domain rdf:resource="https://ontology.swissartresearch.net/aaao/ZE37_Population"/>
    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property relates an instance of sovereignty to a population over which it wielded this sovereignty.</rdfs:comment>
    <rdfs:label xml:lang="en">was subject population of</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WP4i</skos:notation>
    <rdfs:range rdf:resource="https://ontology.swissartresearch.net/pwro/WE2_Sovereignty"/>
