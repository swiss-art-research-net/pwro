    <rdfs:label xml:lang="en">was addressee of</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WP12i</skos:notation>
    <rdfs:range rdf:resource="https://ontology.swissartresearch.net/pwro/WE11_Correspondence_Event"/>
