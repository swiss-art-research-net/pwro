    <rdfs:range rdf:resource="https://ontology.swissartresearch.net/pwro/WE4_Manifest_Group"/>
  </rdf:Property>
  <rdf:Property rdf:about="https://ontology.swissartresearch.net/pwro/WP6i_was_intended_target_of">
